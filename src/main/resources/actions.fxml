<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" prefHeight="125.0" prefWidth="340.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="340.0">
         <Button fx:id="btnInspect" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Inspect">
            <HBox.margin>
               <Insets right="5.0" />
            </HBox.margin>
         </Button>
         <Button fx:id="btnUse" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Use">
            <HBox.margin>
               <Insets left="5.0" right="5.0" />
            </HBox.margin>
         </Button>
         <Button fx:id="btnPickUp" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Pick Up">
            <HBox.margin>
               <Insets left="5.0" />
            </HBox.margin>
         </Button>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </HBox>
      <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Back to Items" fx:id="btnBackToChoseItem">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
