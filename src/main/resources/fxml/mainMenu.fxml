<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<BorderPane stylesheets="@../css/mainMenu.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.stuttgart_hdm.mi.se2.gui.controller.MenuController">
   <center>
      <VBox alignment="CENTER" styleClass="menu-Buttons">
          <Label text="Main Menu" />
          <Button fx:id="startGameBtn" onAction="#startGame" prefWidth="100" text="Start" />
          <Button fx:id="optionBtn" onAction="#option" prefWidth="100" text="Options" />
          <Button fx:id="exitBtn" onAction="#exit" prefWidth="100" text="Exit" />
      </VBox>
   </center>
</BorderPane>
